function x=solucion(t,a,b,F0,omega)
bCuad = b^2;
aCuad = a^2;
bOme = (bCuad-omega^2);
aOme = 2.*a.*omega;
F = F0./(sqrt(bOme^2 + aOme^2));
phi = atan(aOme./bOme);

xp = F.*cos(omega.*t-phi);

c1 = 1 - F.*cos(phi);
c2 = a./(sqrt(bCuad-aCuad)).*(1-(bCuad+omega^2)./(bOme).*F.*cos(phi));

bA = sqrt(b^2-a^2.*t);

x=exp(-a.*t).*(c1.*cos(bA)+c2.*sin(bA))+xp;

end